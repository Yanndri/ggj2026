script/source = "@tool
extends RichTextEffect

var bbcode = \"flicker\"

func _process_custom_fx(char_fx: CharFXTransform) -> bool:
	if int(char_fx.elapsed_time) % 1 == 0:
		char_fx.visible = false
	else:
		char_fx.visible = true

	return true
"

